package aplication;

import java.util.ArrayList;
import java.util.List;



public class ProgramaList {

	public static void main(String[] args) {


		List<String > list = new ArrayList();
		
		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		list.add("Ana");
		list.add(2,"Marco");
		
		list.remove("Ana");
		
		System.out.println(list.size());
		System.out.println("-----------------------------");
		
		for(String x: list) {
			System.out.println(x);
		}
		System.out.println("-----------------------------");

		
	}

}
