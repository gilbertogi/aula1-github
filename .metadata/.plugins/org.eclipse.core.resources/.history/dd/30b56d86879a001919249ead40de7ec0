package aplication;

import java.util.ArrayList;
import java.util.List;



public class ProgramaList {

	public static void main(String[] args) {


		List<String > list = new ArrayList();
		
		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		list.add("Ana");
		list.add("Marco");
		
		System.out.println(list.size());
		for(int i=0; i<4; i++) {
			System.out.println(list);
		}
		

	}

}
