import java.util.Scanner;

public class exercico4 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
	    
	    int [] vetorA = new int [15];
		double [] vetorB = new int [vetorA.length]; // vetorB vai ser um double por causa de raiz quadrado
		
		
	 for( int i=0; i<vetorA.length; i++) {
		 System.out.println("Entre com o valor da posuição " + i);
		 vetorA[i]=scan.nextInt();
		 vetorB[i]=Math.sqrt(vetorA[i]); // riaz de vetorA
	 }
	   System.out.print(" Vetor A : ");
	   for (int i=0; i<vetorA.length; i++) {
		   System.out.print(vetorA[i] + " ");
	   }
	   System.out.println();
	   System.out.print("Vetor B : ");
	   for ( int i=0; i<vetorB.length; i ++) {
		   System.out.print(vetorB[i]+ " ");
	   }



		}

	}
