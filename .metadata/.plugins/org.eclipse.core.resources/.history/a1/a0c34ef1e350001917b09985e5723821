package com.gilberto.curso.basico;



public class wh {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		for(int i=0; i<=5; i++) {
		
		System.out.println(i);

	}
		System.out.println("_____________________________");
		for(int i=5; i>=0; i--) {
			
			System.out.println(i);
		}
		
		
	}
}
