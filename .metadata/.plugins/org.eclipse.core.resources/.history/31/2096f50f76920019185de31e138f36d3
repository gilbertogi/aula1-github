
public class Lampada {
	
	String modelo;
	String tensao;
	int potencia;
	String cor;
	String tipoLuz;
	int garantiaMeses;
	String [] tipos;
	boolean tipoAbajur;
	
	
	boolean ligado;
	
	
	void ligar() {
		ligado=true;
	}
	void desligar() {
		ligado=false;
		}
	
	void mostrarStatus() {
		if(ligado) {
			System.out.println("Lampada está ligado");
		} else {
			System.out.println("Lampda está desligado");
		}
	}
	}


