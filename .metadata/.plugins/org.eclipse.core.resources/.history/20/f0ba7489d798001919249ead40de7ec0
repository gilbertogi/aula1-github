package Brasileiro;

import java.util.Locale;
import java.util.Scanner;

public class Vetor {

	public static void main(String[] args) {


		Locale.setDefault(Locale.US);
		
		Scanner sc= new Scanner (System.in);
		int n=5;
		double [] vetor= new double[n];
		
		for( int i=0; i<n; i++) {

			 
			vetor[i]=n;
		}
		double soma=0;
		for( int i=0; i<n; i++) {
		soma+=vetor[i]	;
		}
		
		double media= soma/n;
		System.out.println();
		System.out.println(media);
		
		sc.close();

	}

}
