
public class Lampada {
	
	String modelo;
	String tensao;
	int potencia;
	String cor;
	String tipoLuz;
	int garantiaMeses;
	String [] tipos;
	boolean tipoAbajur;
	
	
	boolean ligado;
	
	
	void ligadoLampada() {
		if(this.ligado) {
			System.out.println("A lampdada esta ligado");
		}
	}
	void desligadoLampda() {
		if(this.ligado==false) {
			System.out.println("A Lampada está desligado");
		}
	}

}
